<?xml version="1.0" encoding="UTF-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ConfigLayoutSchema.xsd">
    <hlayout id="hlayout01">
        <vlayout id="vlayout01" border="true">
            <title>title01</title>
            
            <toggle id="enableQualityToggle">
                <contentId>toggle01</contentId>
                <tooltip>tooltip01</tooltip>
            </toggle>

            <toggle id="enableLevelToggle">
                <contentId>toggle02</contentId>
                <tooltip>tooltip02</tooltip>
            </toggle>
        </vlayout>

        <vlayout id="vlayout02" border="true">
            <title>title02</title>

            <toggle id="enableVanillaLevelScalingToggle">
                <contentId>toggle03</contentId>
                <tooltip>tooltip03</tooltip>
            </toggle>
            <topic>topic01</topic>
            <slider id="slider01" min="1" max="9999" buttons="true">0</slider>
            <topic>topic02</topic>
            <one_choice id="dropdown01" type="dropdown">
                <choice>
                    <contentId>1</contentId>
                </choice>
                <choice>
                    <contentId>10</contentId>
                </choice>
                <choice>
                    <contentId>100</contentId>
                </choice>
                <choice>
                    <contentId>1000</contentId>
                </choice>
            </one_choice>
        </vlayout>
    </hlayout>
</config>